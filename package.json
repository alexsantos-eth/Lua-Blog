{
	"name": "lua-blog-static",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"firebase": "^7.17.1",
		"html-react-parser": "^0.13.0",
		"prettier": "1.19.1",
		"prismjs": "^1.21.0",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-feather": "2.0.8",
		"react-helmet": "^6.1.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "5.0.1"
	},
	"devDependencies": {
		"@arcadian-digital/react-snap": "^1.23.2",
		"@types/node": "^12.0.0",
		"@types/prismjs": "^1.16.1",
		"@types/react": "^16.9.0",
		"@types/react-dom": "^16.9.0",
		"@types/react-helmet": "^6.1.0",
		"@types/react-router-dom": "^5.1.5",
		"axios": "^0.19.2",
		"node-sass": "^4.14.1",
		"source-map-explorer": "2.0.1",
		"ts-node": "^8.10.2",
		"tslint": "^6.1.3",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.3.0",
		"tslint-react": "^5.0.0",
		"typescript": "~3.9.7"
	},
	"scripts": {
		"start": "react-scripts start",
		"analyze": "source-map-explorer 'build/static/js/*.js'",
		"build": "npm run fetch && react-scripts build",
		"deploy": "npm run build && react-snap",
		"build:analyze": "react-scripts build && npm run analyze",
		"test": "react-scripts test",
		"fetch": "ts-node fetchData.ts",
		"eject": "react-scripts eject",
		"config": "yarn add --dev prettier react-snapshot tslint tslint-config-prettier tslint-plugin-prettier tslint-react node-sass"
	},
	"reactSnap": {
		"skipThirdPartyRequests": true
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
